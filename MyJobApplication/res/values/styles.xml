<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="application_theme">
        <item name="android:background">@color/application_background_color</item>
    </style>
    <style name="button_theme1">
        <item name="android:background">@color/primary_button_color</item>
        <item name="android:textColor">@color/primary_button_text_color</item>
        <item name="android:textSize">@dimen/primary_button_text_size</item>
        <item name="android:paddingStart">@dimen/text_button_padding</item>
        <item name="android:paddingEnd">@dimen/text_button_padding</item>
        <item name="android:paddingTop">@dimen/text_button_padding</item>
        <item name="android:paddingBottom">@dimen/text_button_padding</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="text_label_style1">
        <item name="android:textSize">@dimen/text_label_size</item>
        <item name="android:layout_marginBottom">@dimen/text_entry_margin</item>
    </style>
    <style name="text_entry_style1">
        <item name="android:background">@color/text_entry_background</item>
        <item name="android:textSize">@dimen/text_entry_size</item>
        <item name="android:layout_marginBottom">@dimen/text_entry_margin</item>
    </style>
    <style name="text_value_style1">
        <item name="android:textSize">@dimen/text_entry_size</item>
        <item name="android:layout_marginBottom">@dimen/text_entry_margin</item>
    </style>
    <style name="list_entry_style">
        <item name="android:textColor">@color/primary_button_text_color</item>
        <item name="android:textSize">@dimen/text_list_entry_size</item>
        <item name="android:padding">5dp</item>
    </style>
    <style name="list_entry_label_style">
        <item name="android:textColor">@color/label_color</item>
        <item name="android:textSize">@dimen/text_list_entry_size</item>
        <item name="android:padding">5dp</item>
    </style>
</resources>